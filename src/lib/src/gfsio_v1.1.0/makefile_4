LIB     = $(ESM_HOME)/src/lib/libgfsio_4.a
INCMOD  = $(ESM_HOME)/src/lib/incmod/gfsio_4
FC      = $(FCS)
FFLAGS  = -traceback -g -xHOST -convert big_endian -assume byterecl  -I$(INCMOD) -FR
AR      = ar
ARFLAGS = -rv 

$(LIB): $(LIB)(gfsio_module.o) 
.f.a:
	$(FC) -c $(FFLAGS) $<
	$(AR) $(ARFLAGS) $@ $*.o
	rm -f $*.o
	mv $*.mod $(INCMOD)

